extends layout 
block content 
    w3.card-4 
        header.w3-container.w3-teal 
            h1= contrato["nAnuncio"]
            h2= contrato._id
        .w3-container 
            p Tipo do procedimento: #{contrato.tipoprocedimento}
            p Objecto do Contrato: #{contrato.objectoContrato}
            p Data de Publicacao: #{contrato.dataPublicacao}
            p Data de Celebracao do Contrato: #{contrato.dataCelebracaoContrato}
            p Preco Contratual: #{contrato.precoContratual}
            p Prazo de Execucao: #{contrato.prazoExecucao}
            p NIPC da entidade comunicante: #{contrato.NIPC_entidade_comunicante}
            p Entidade comunicante: #{contrato.entidade_comunicante}
            p Fundamentacao: #{contrato.fundamentacao}
            button 
                a(href='/') Inicio             
        
        footer.w3-container.w3-teal 
            h5 Generated in #{d}
            